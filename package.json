{
	"name": "eleventy-base-blog",
	"version": "9.0.0",
	"description": "A starter repository for a blog web site using the Eleventy site generator.",
	"type": "module",
	"scripts": {
		"scss:build": "sass scss/index.scss css/index.css --style=compressed",
		"scss:watch": "sass scss/index.scss css/index.css --watch --style=compressed",
		"eleventy:build": "npx @11ty/eleventy",
		"eleventy:watch": "npx @11ty/eleventy --serve --quiet",
		"build": "npm-run-all scss:build eleventy:build",
		"start": "npm-run-all scss:build --parallel scss:watch eleventy:watch",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npm run build",
		"build-ghpages": "npm-run-all scss:build \"eleventy:build --pathprefix=/eleventy-base-blog/\"",
		"start-ghpages": "npm-run-all scss:build --parallel scss:watch \"eleventy:watch --pathprefix=/eleventy-base-blog/\"",
		"debug": "cross-env DEBUG=Eleventy* npm run eleventy:build",
		"debugstart": "npm-run-all scss:build --parallel scss:watch \"cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet\"",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npm run eleventy:build"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/11ty/eleventy-base-blog.git"
	},
	"author": {
		"name": "Zach Leatherman",
		"email": "zachleatherman@gmail.com",
		"url": "https://zachleat.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"bugs": {
		"url": "https://github.com/11ty/eleventy-base-blog/issues"
	},
	"homepage": "https://github.com/11ty/eleventy-base-blog#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
		"cross-env": "^7.0.3",
		"luxon": "^3.6.1",
		"netlify-plugin-cache": "^1.0.3",
		"npm-run-all": "^4.1.5",
		"prismjs": "^1.30.0",
		"sass": "^1.97.3",
		"zod": "^3.25.67",
		"zod-validation-error": "^3.5.2"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.5"
	}
}
